<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarView">
        <!--monthTitle 2019年1月-->
        <attr name="isShowMonthTitleView" format="boolean" />
        <!--monthView-->
        <attr name="textColor" format="color" />
        <attr name="monthPaddingLeft" format="dimension" />
        <attr name="monthPaddingTop" format="dimension" />
        <attr name="monthPaddingRight" format="dimension" />
        <attr name="monthPaddingBottom" format="dimension" />
        <attr name="monthMarginLeft" format="dimension" />
        <attr name="monthMarginTop" format="dimension" />
        <attr name="monthMarginRight" format="dimension" />
        <attr name="monthMarginBottom" format="dimension" />
        <attr name="selectStyle" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="roundRectangle" value="1" />
        </attr>
        <attr name="cornerRadius" format="dimension" />
        <attr name="selectTextColor" format="color" />
        <attr name="weekendTextColor" format="color" />
        <attr name="selectBgColor" format="color" />
        <attr name="disTextColor" format="color" />
        <attr name="sameTextColor" format="color" />
        <attr name="preTextColor" format="color" />
        <attr name="topTextColor" format="color" />
        <attr name="selectRangebgColor" format="color" />
        <attr name="selectMaxRange" format="integer" />
        <attr name="textSize" format="dimension" />
        <attr name="topTextSize" format="dimension" />
        <attr name="bottomTextSize" format="dimension" />
        <attr name="firstTopMargin" format="dimension" />
        <attr name="secondTopMargin" format="dimension" />
        <attr name="thirdTopMargin" format="dimension" />
        <attr name="dividerHeight" format="dimension" />
        <attr name="dividerColor" format="color" />
        <attr name="firstSelectDayText" format="string" />
        <attr name="lastSelectDayText" format="string" />
    </declare-styleable>


    <declare-styleable name="MonthLableView">
        <attr name="lableWeekendTextColor" format="color" />
        <attr name="lableTextColor" format="color" />
        <attr name="lableTextSize" format="dimension" />
    </declare-styleable>

</resources>